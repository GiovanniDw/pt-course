<h3>Games</h3>
<div>
    <ul class="games">
   
        <% for (var index = 0; index < games.length; index++) { %>
        <li class="item">
        
            <img src="<%= games[index].cover %>" alt="<%= games[index].title %>">
          
        </li>
        <% } %>
         <li class="item">
            <img src="images/add-game.png" alt="">
            <a href='/games' id="" class="icon add material-icons">search</a>
        </li>
    </ul>    
</div>
